<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder class="ch.qos.logback.core.encoder.LayoutWrappingEncoder">
      <layout class="ch.qos.logback.classic.PatternLayout">
        <Pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</Pattern>
      </layout>
    </encoder>
  </appender>

  <root level="warn">
    <appender-ref ref="STDOUT" />
  </root>

  <!--<logger name="com.opengamma.sesame.engine.Engine" level="debug"/>-->
  <logger name="com.opengamma.sesame.fxforward.FXForwardPVFunctionTest" level="info"/>
  <!--<logger name="com.opengamma.sesame.cache.CachingProxyDecorator" level="debug"/>-->
  <!--<logger name="com.opengamma.sesame.cache.ProxyCacheLoader" level="debug"/>-->
  <!--<logger name="com.opengamma.sesame.cache.CachingProxyDecorator$EntryFactory" level="debug"/>-->

</configuration>
